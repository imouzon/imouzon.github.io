\textbf{Highlights}

%-- : R code (Code in Document)
<<echo=FALSE, cache=FALSE, include=TRUE>>=
   Qfun <- function(x,p = NULL){
      x <- x[order(x)]

      n <- length(x)

      qs <- ((1:n) - .5)/n

      if(is.null(p)) return(qs)

      i <- floor(n*p + .5)

      Qp <- x[i] + (n*p + 0.5 - i)*(x[i+1] - x[i])

      return(Qp)
   }

   IQRange <- function(x) Qfun(x,.75) - Qfun(x,.25)

   samp <- rnorm(26,5,2)
   samp <- samp[order(samp)]

   samp[25:26] = max(Qfun(samp,.75) + 1.5*IQRange(samp),samp[25]) + runif(2)

   samp[1] = 1*(Qfun(samp,.21) - 1.5*IQRange(samp))/3

   samp <- round(samp,1)

   samp <- pmax(samp,0.1)
   
@

